{
  "expo": {
    "name": "CovidBG",
    "slug": "covidbgcom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#073642"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "me.parallela.covidbgcom"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#073642"
      },
      "package": "me.parallela.covidbgcom"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "The mobile version of the website covidbulgaria.com",
    "githubUrl": "https://github.com/parallela/CovidBulgaria-Mobile"
  }
}
